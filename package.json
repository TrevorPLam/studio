    "test:e2e": "jest --testPathPattern=e2e",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\" \"*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\" \"*.{ts,tsx,json,md}\"",
